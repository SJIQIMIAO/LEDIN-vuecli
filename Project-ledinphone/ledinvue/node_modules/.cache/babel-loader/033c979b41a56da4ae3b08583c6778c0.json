ARA,GAPA;AAiBA,SAjBA,qBAiBA;AACA;AACA;AACA,SAAA,aAAA;AACA,GArBA;AAsBA;AACA,cAAA;AACA;AACA,mBAAA;AAFA;AAvBA,CAAA","sourcesContent":["<template>\r\n    <div class=\"app_newsdetail\">\r\n        <!-- 新闻详情页没有现成的组件,自己写 -->\r\n        <h3 class=\"title\">{{newsinfo.title}}</h3>\r\n        <p class=\"subtitle\">\r\n            <span>发表时间:{{newsinfo.ctime | dateFormat}}</span>\r\n            <span>点击:{{newsinfo.click}}次</span>\r\n\r\n        </p>\r\n         <img :src=\"newsinfo.img_url\">\r\n        <div class=\"container\">{{newsinfo.content}}</div>\r\n        \r\n        <!-- 评论区域 一个单独的子组件-->\r\n        <comment-box :id=\"this.id\"></comment-box>\r\n    </div>\r\n</template>\r\n<script>\r\n//1. 引入评论子组件\r\n    import comment from \"../subcomponents/comment\";\r\nexport default { \r\n    data(){\r\n        return{\r\n            id:this.$route.params.id,//使底下路径中的id动态生成\r\n            newsinfo:{}//保存当前的新闻内容\r\n        }\r\n    },\r\n    methods:{\r\n        getNewsDetail(){\r\n            //获取新闻的详情内容\r\n            var url=\"newslist/find?id=\"+this.id;\r\n            this.$http.get(url).then(result=>{\r\n                this.newsinfo=result.body.msg[0];\r\n                // console.log(this.newsinfo);\r\n            });\r\n        }\r\n    },\r\n    created(){\r\n        // console.log(this.$route.params.id);\r\n        //调用方法\r\n        this.getNewsDetail();\r\n    },\r\n    //1. 配置评论子组件\r\n    components:{\r\n        // 自己取得名字\r\n        'comment-box':comment\r\n    }\r\n}\r\n</script>\r\n<style>\r\n    .app_newsdetail{\r\n        padding:0 4px;\r\n    }\r\n    .app_newsdetail .title{\r\n        font-size:15px;\r\n        text-align:center;\r\n        margin:15px 0;\r\n    }\r\n    .app_newsdetail .subtitle{\r\n        font-size:11px;\r\n        display:flex;\r\n        justify-content:space-between;\r\n    }\r\n    .app_newsdetail img{\r\n        width:100%;\r\n    }\r\n</style>\r\n\r\n"],"sourceRoot":"src\\components\\news"}]}