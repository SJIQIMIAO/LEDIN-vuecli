ass=\"container\" v-html=\"newsinfo.content\">\r\n            <img :src=\"newsinfo.img_url\" alt=\"\">\r\n            </div>\r\n        \r\n        <!-- 评论区域 一个单独的子组件-->\r\n    </div>\r\n</template>\r\n<script>\r\nexport default {\r\n    data(){\r\n        return{\r\n            id:this.$route.params.id,//使底下路径中的id动态生成\r\n            newsinfo:{}//保存当前的新闻内容\r\n        }\r\n    },\r\n    methods:{\r\n        getNewsDetail(){\r\n            //获取新闻的详情内容\r\n            var url=\"newslist/find?id=\"+this.id;\r\n            this.$http.get(url).then(result=>{\r\n                this.newsinfo=result.body.msg[0];\r\n                console.log(this.newsinfo);\r\n            });\r\n        }\r\n    },\r\n    created(){\r\n        console.log(this.$route.params.id);\r\n        //调用方法\r\n        this.getNewsDetail();\r\n    }\r\n}\r\n</script>\r\n<style>\r\n    .app_newsdetail{\r\n        padding:0 4px;\r\n    }\r\n    .app_newsdetail .title{\r\n        font-size:15px;\r\n        text-align:center;\r\n        margin:15px 0;\r\n    }\r\n    .app_newsdetail .subtitle{\r\n        font-size:11px;\r\n        display:flex;\r\n        justify-content:space-between;\r\n    }\r\n    .app_newsdetail .container img{\r\n        width:100%;\r\n    }\r\n</style>\r\n\r\n"]}]}